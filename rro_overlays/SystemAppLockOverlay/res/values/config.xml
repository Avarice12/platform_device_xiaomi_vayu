        <item>com.google.android.apps.maps</item>
        <!-- Messaging -->
        <item>com.google.android.apps.messaging</item>
        <!-- Play store -->
        <item>com.android.vending</item>
        <!-- Photos -->
        <item>com.google.android.apps.photos</item>
